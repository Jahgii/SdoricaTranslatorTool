{
  "name": "sdoricatranslatortool",
  "version": "0.0.0",
  "main": "main.js",
  "author": "Jahg",
  "description": "Sdorica Translator Tool",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44415 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44415 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build --base-href ./ && electron .",
    "dist": "ng build --base-href ./ && electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.4",
    "@angular/cdk": "^20.2.8",
    "@angular/common": "^20.3.4",
    "@angular/compiler": "^20.3.4",
    "@angular/core": "^20.3.4",
    "@angular/forms": "^20.3.4",
    "@angular/platform-browser": "^20.3.4",
    "@angular/platform-browser-dynamic": "^20.3.4",
    "@angular/platform-server": "^20.3.4",
    "@angular/router": "^20.3.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@floating-ui/dom": "^1.6.5",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@taiga-ui/addon-doc": "^4.49.0",
    "@taiga-ui/addon-mobile": "^4.49.0",
    "@taiga-ui/addon-table": "^4.49.0",
    "@taiga-ui/cdk": "^4.49.0",
    "@taiga-ui/core": "^4.49.0",
    "@taiga-ui/event-plugins": "^4.2.3",
    "@taiga-ui/experimental": "^4.49.0",
    "@taiga-ui/icons": "^4.49.0",
    "@taiga-ui/kit": "^4.49.0",
    "@taiga-ui/layout": "4.49.0",
    "@taiga-ui/legacy": "4.49.0",
    "@taiga-ui/styles": "^4.49.0",
    "dompurify": "2.2.9",
    "dotenv": "^16.4.5",
    "electron-store": "^8.2.0",
    "jszip": "^3.10.1",
    "ngx-device-detector": "^10.1.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.1",
    "shepherd.js": "^14.5.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular/build": "^20.3.5",
    "@angular/cli": "^20.3.5",
    "@angular/compiler-cli": "^20.3.4",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.19.22",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.9.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "autoprefixer": "10.4.5",
    "webpack": "5.81.0"
  },
  "build": {
    "appId": "Jahg.STT",
    "productName": "Sdorica Translator Tool",
    "asar": true,
    "directories": {
      "output": "dist/release/"
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "Stt.exe"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.map",
      "!package.json",
      "!package-lock.json",
      {
        "from": "../dist",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
