{
  "name": "sdoricatranslatortool",
  "version": "0.0.0",
  "main": "main.js",
  "author": "Jahg",
  "description": "Sdorica Translator Tool",
  "scripts": {
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "run-script-os",
    "start:windows": "ng serve --port 44415 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\"",
    "start:default": "ng serve --port 44415 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\"",
    "build": "ng build",
    "build:ssr": "ng run SdoricaTranslatorTool:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build --base-href ./ && electron .",
    "dist": "ng build --base-href ./ && electron-builder"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "^2.2.0",
    "@angular/animations": "^18.1.1",
    "@angular/cdk": "^17.2.2",
    "@angular/common": "^18.1.1",
    "@angular/compiler": "^18.1.1",
    "@angular/core": "^18.1.1",
    "@angular/forms": "^18.1.1",
    "@angular/platform-browser": "^18.1.1",
    "@angular/platform-browser-dynamic": "^18.1.1",
    "@angular/platform-server": "^18.1.1",
    "@angular/router": "^18.1.1",
    "@auth0/angular-jwt": "^5.2.0",
    "@floating-ui/dom": "^1.6.5",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@taiga-ui/addon-mobile": "^3.69.0",
    "@taiga-ui/addon-preview": "^3.69.0",
    "@taiga-ui/addon-table": "^3.69.0",
    "@taiga-ui/addon-tablebars": "^3.69.0",
    "@taiga-ui/cdk": "^3.69.0",
    "@taiga-ui/core": "^3.69.0",
    "@taiga-ui/icons": "^3.69.0",
    "@taiga-ui/kit": "^3.69.0",
    "@taiga-ui/layout": "^3.69.0",
    "@taiga-ui/styles": "^3.69.0",
    "@tinkoff/ng-dompurify": "3.0.0",
    "angular-shepherd": "^17.0.0",
    "dompurify": "2.2.9",
    "dotenv": "^16.4.5",
    "jszip": "^3.10.1",
    "ngx-device-detector": "^7.0.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.8.1",
    "shepherd.js": "^12.0.6",
    "tslib": "^2.5.0",
    "zone.js": "^0.14.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.1",
    "@angular-builders/dev-server": "^7.3.1",
    "@angular-devkit/build-angular": "^18.1.1",
    "@angular/cli": "^18.1.1",
    "@angular/compiler-cli": "^18.1.1",
    "@types/dompurify": "2.2.3",
    "@types/jasmine": "~4.3.1",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.19.22",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.5.4"
  },
  "overrides": {
    "autoprefixer": "10.4.5",
    "webpack": "5.81.0"
  },
  "build": {
    "appId": "Jahg.STT",
    "productName": "Sdorica Translator Tool",
    "asar": true,
    "directories": {
      "output": "dist/release/"
    },
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "Stt.exe"
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.map",
      "!package.json",
      "!package-lock.json",
      {
        "from": "../dist",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
